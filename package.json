{
  "name": "birthday-slider-generator",
  "description": "Star organizer in Github",
  "author": "",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-life/birthday-slider-generator"
  },
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot",
    "react-hot-reloader",
    "redux",
    "babel"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrent --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "dev": "concurrent --kill-others \"npm run start-dev\" \"npm run start-dev-api\"",
    "start:linter": "better-npm-run start-linter",
    "start-prod": "better-npm-run start-prod",
    "start-prod-api": "better-npm-run start-prod-api",
    "start-dev": "better-npm-run start-dev",
    "start-dev-api": "better-npm-run start-dev-api",
    "build": "better-npm-run build",
    "server": "better-npm-run server",
    "lint": "eslint -c .eslintrc src bin webpack",
    "lint:fix": "eslint --fix -c .eslintrc src bin webpack",
    "stylelint": "stylelint src/components/**/*.sss src/styles/**/*.sss",
    "ava": "better-npm-run ava",
    "test": "nyc --all npm run ava",
    "test:watch": "npm run ava -- --watch",
    "storybook": "better-npm-run start-storybook",
    "build-storybook": "better-npm-run build-storybook",
    "make": "babel-node make-block.js"
  },
  "ava": {
    "require": [
      "babel-register",
      "./webpack/hook-test"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "require": "babel-register",
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/*.js",
      "src/redux/create.js",
      "src/redux/modules/reducer.js",
      "src/**/*.test.js",
      "src/**/*.story.js",
      "src/**/index.js",
      "src/helpers/*.js",
      "static/**/*.js"
    ]
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "APIPORT": 3030
      }
    },
    "start-prod-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production",
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3031
      }
    },
    "start-dev-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "APIPORT": 3031
      }
    },
    "start-linter": {
      "command": "npm run start",
      "env": {
        "LINTER": true
      }
    },
    "build": {
      "command": "webpack --colors --verbose --display-error-details --config webpack/prod.config.babel.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "server": {
      "command": "node --harmony ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "APIPORT": 3031
      }
    },
    "ava": {
      "command": "ava --verbose",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "test"
      }
    },
    "start-storybook": {
      "command": "start-storybook -p 9001",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build-storybook": {
      "command": "build-storybook -o static/__components",
      "env": {
        "NODE_ENV": "storybook"
      }
    }
  },
  "dependencies": {
    "autoprefixer": "^6.4.0",
    "axios": "^0.11.0",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.3.19",
    "better-npm-run": "^0.0.10",
    "body-parser": "^1.15.2",
    "classnames": "^2.2.3",
    "compression": "^1.6.0",
    "cookie-parser": "^1.4.2",
    "css-loader": "^0.23.1",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.8.5",
    "hoist-non-react-statics": "^1.0.3",
    "http-proxy": "^1.14.0",
    "json-loader": "^0.5.4",
    "moment": "^2.14.1",
    "postcss-autoreset": "^1.1.5",
    "postcss-color-function": "^2.0.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-font-magician": "^1.4.0",
    "postcss-import": "8.0.2",
    "postcss-loader": "^0.9.1",
    "postcss-nested": "^1.0.0",
    "pretty-error": "^2.0.0",
    "pure-render-decorator": "^1.1.1",
    "ramda": "^0.22.1",
    "raw-loader": "^0.5.1",
    "react": "^15.3.0",
    "react-addons-perf": "^15.3.0",
    "react-cookie": "^0.4.7",
    "react-css-modules": "^3.7.10",
    "react-dom": "^15.3.0",
    "react-helmet": "^3.1.0",
    "react-modal": "^1.4.0",
    "react-redux": "^4.4.0",
    "react-router": "^3.0.0-alpha.3",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-act": "^0.5.0",
    "redux-async-connect": "^1.0.0-rc4",
    "redux-form": "^6.0.0-rc.4",
    "redux-loop": "^2.2.2",
    "scroll-behavior": "^0.3.2",
    "socket.io": "^1.4.8",
    "style-loader": "^0.13.1",
    "sugarss": "^0.1.5",
    "url-loader": "^0.5.7"
  },
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.2.1",
    "@kadira/storybook": "^2.5.1",
    "ava": "^0.16.0",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-typecheck": "^3.6.0",
    "chai": "^3.3.0",
    "clean-webpack-plugin": "^0.1.6",
    "concurrently": "^0.1.1",
    "enzyme": "^2.4.1",
    "eslint": "3.3.0",
    "eslint-config-airbnb": "10.0.0",
    "eslint-import-resolver-node": "^0.2.2",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "6.1.1",
    "husky": "^0.11.6",
    "invariant": "^2.2.1",
    "mock-css-modules": "^1.0.0",
    "nyc": "^8.1.0",
    "piping": "^1.0.0-rc.3",
    "react-addons-test-utils": "^15.3.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "stylelint": "^7.1.0",
    "webpack": "^2.1.0-beta.20",
    "webpack-dev-middleware": "^1.6.0",
    "webpack-hot-middleware": "^2.12.0"
  },
  "engines": {
    "node": "6.3.0"
  }
}
